<h3><%= t('titles.upload_a_photo') %></h3>
<%= form_for [@event, photo], html: { multipart: true } do |f| %>
  <%= render partial: 'partials/form_errors', locals: { model: photo } %>

  <div class="form-group">
    <%= f.label :photo %>
    <%= f.file_field :photo %>
  </div>

  <br/>

  <div class="actions">
    <%= f.submit t('button.upload'), class: 'btn btn-primary' %>
  </div>
<% end %>
